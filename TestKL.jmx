              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">def requestBody = prev.getResponseDataAsString();
def numbers =[]
def m = requestBody =~/&lt;label .+&gt;(\d{1,3})/
(0..&lt;m.count).each{
	numbers.add(m[it][1].toInteger())}
def maxNum = numbers.max();
vars.put(&quot;max_value&quot;,maxNum.toString())
def challenger_order_selected = (requestBody =~/type=&quot;radio&quot;.*value=&quot;(.*?)&quot;/)[numbers.indexOf(maxNum)][1]
vars.put(&quot;radio&quot;,challenger_order_selected)
</stringProp>
